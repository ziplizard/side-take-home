// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Property {
  id        Int     @id @default(autoincrement())
  address   String
  price     Decimal
  bedrooms  Int // smallint
  bathrooms Int // smallint
  type      String?
}
